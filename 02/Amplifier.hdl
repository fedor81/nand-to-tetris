/**
 * A amplifier from 1-bit to 16-bit
 */

CHIP Amplifier {
    IN in;
    OUT out[16];

    PARTS:
    Not(in=in, out=notIn);
    Not(in=notIn, out=out[0], out=out[1], out=out[2], out=out[3], out=out[4], out=out[5], out=out[6], out=out[7], out=out[8], out=out[9], out=out[10], out=out[11], out=out[12], out=out[13], out=out[14], out=out[15]);
}